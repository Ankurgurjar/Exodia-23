<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Exodia | Team</title>

    <!-- cdn for css of bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/navbar.css">
    <link rel="stylesheet" href="/css/profile.css">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    <link rel="stylesheet" href="/css/scrollbar.css">
    <link rel="icon" type="image/x-icon" href="/images/exodia_logo.png">
</head>

<body>

    <!-- importing navbar from the partials -->
    <%- include('partials/navbar.ejs') %>
        <%- include('partials/clip_path_svgs.ejs') %>

            <div class="profile">

                <div class="image-wrapper">

                    <div class="user-wrapper">
                        <div id="user-img" data-aos="fade-right" data-aos-easing="linear" data-aos-duration="500">

                            <svg width="110px" height="110px" viewBox="0 0 108.43 108.66" role="presentation"
                                aria-hidden="true" class="">
                                <defs>
                                    <style>
                                        .roundel-a {
                                            fill: #44997E;
                                            opacity: 0.3
                                        }

                                        .roundel-b {
                                            fill: #44997E;
                                            opacity: 0.3
                                        }

                                        .roundel-c {
                                            fill: #44997E;
                                            opacity: 0.3
                                        }

                                        .roundel-d {
                                            opacity: 1;
                                            fill: url(#rdl-profile-v2-avatar-image)
                                        }
                                    </style>
                                    <pattern id="rdl-profile-v2-avatar-image" width="100" height="100"
                                        patternUnits="userSpaceOnUse" viewBox="0 0 100 100">
                                        <image
                                            xlink:href="https://blog.vantagecircle.com/content/images/2020/08/teamwork-and-team-building.png"
                                            width="100" height="100"></image>
                                    </pattern>
                                </defs>
                                <path class="roundel-a"
                                    d="M29.15,99l-14.75-13c.43-1.72,1.21-3,.87-3.91-.4-1-1.71-1.8-2.73-2.53S10.07,78,8.85,77.25C2.33,73.15-.08,66.42,3.26,59.5c1.47-3,.6-5.34-.73-7.48-1.68-2.7-1-4.57,1.1-6.19,4.2-3.31,3.53-7.47,1.48-11.2-1.94-3.52-1.55-6.26.57-9.27,6.81-9.69,16.38-13.54,27.83-12.46,3.87.37,6.2-.83,7.86-3.83C46.5-.23,50.15,1,57.43,6c5.31,3.62,10.33,7.52,17,8.37a5.79,5.79,0,0,1,2.86,1.59C83.88,22,90.35,28.2,94.94,36c2.84,4.84,5.67,9.69,3.27,15.71-.33.83.42,2.08.64,3.13.38,1.8.74,3.6,1.09,5.32a7.89,7.89,0,0,1-1.76.88c-1.69.21-4-.27-4.21,2.09-.16,1.93.57,4,1.15,5.88.19.64,1.13,1.08,1.77,1.56,7.28,5.42,5,17-2.82,20-3.48,1.31-6.45,3.64-10.7,2.7-1.74-.38-4.61.86-5.79,2.31-1.6,2-3.2,2.62-5.31,2.11-3.81-.93-6.44.82-8.94,3.29-5.35,5.28-12.89,4.91-17.38-1-2.21-2.88-4.77-4-8.34-3C34.58,97.9,31.49,98.44,29.15,99Z">
                                </path>
                                <path class="roundel-b"
                                    d="M103,52.95c0,12.6-1.82,20.21-9.28,25.59C92,79.82,90.9,82.3,90,84.43c-2.65,6.15-5.28,12.15-13.61,11.49-8.42,9.75-18.55,6.55-28.76,4.05-2.2-.54-4.82.46-7.21.94-16.23,3.22-36.2-13-36.94-30C2.87,57.5,1,44.17,0,30.79-.75,20.5,14.37,4.17,27.09,3.77c11.58-.37,23.29,2,34.87,3.6a32.94,32.94,0,0,1,11.4,4.18c8,4.61,18.45,8,22.92,15.1C101.57,35,101.62,46.66,103,52.95Z">
                                </path>
                                <path class="roundel-c"
                                    d="M52.14,106.26c-25.1,0-41.49-9.8-49.4-33.19-3-8.74-.9-19.65.55-29.31C6.34,23.44,23.83,5.15,42.61,1,51-.89,58.9-.4,65.91,5.2,75.52,12.89,86,19.78,94.4,28.59c4.17,4.36,6.06,12.17,6.48,18.58,1,15.92-4.38,30.77-12.59,44C80.24,104.12,66.37,106.39,52.14,106.26Z">
                                </path>
                                <path class="roundel-d __web-inspector-hide-shortcut__"
                                    d="M94.5,51.26c0,21.44-8.37,35.44-28.35,42.2-7.47,2.53-16.78.77-25-.47a39.76,39.76,0,0,1-8.49-2.28c-21.94-9-30.94-36-22.94-58A50.12,50.12,0,0,1,17.6,21.61a50.14,50.14,0,0,1,7.48-5.95,45.74,45.74,0,0,1,17.6-6.92c5-1,9,1,13.88,1.32q3.42.65,6.73,1.63a63.2,63.2,0,0,1,18.3,8.68,24.81,24.81,0,0,1,6.7,6C93.51,33.2,94.58,42.14,94.5,51.26Z">
                                </path>
                            </svg>

                            <!-- <img src="/images/cat.jpeg"> -->
                        </div>

                        <div id="user-name">
                            <h4>
                                <%= team.name%>
                            </h4>
                        </div>

                    </div>

                    <div class='btn-wrapper'>
                        <button class="btn-between" id="members"><a href="#members_panel">Team Members</a></button>
                        <button class="btn-between" id="referral"><a href="#referral_panel">Refferal ID</a></button>
                        <button id="achievements"><a href="#achievements_panel">Achievements</a></button>
                    </div>
                </div>

                <div class="info-wrapper">
                    <div class="content-info vis" id="members_panel">
                        <div class="contactedit">
                            <!-- <img style="width:100px" src="/giphy.gif" alt=""> -->
                            <div class="h1editleft" data-aos="fade-right" data-aos-easing="linear"
                                data-aos-duration="500"></div>
                            <h1 data-aos="zoom-in" data-aos-easing="linear" data-aos-duration="500">
                                Team Members
                            </h1>
                            <div class="h1editright" data-aos="fade-left" data-aos-easing="linear"
                                data-aos-duration="500"></div>
                        </div>

                        <ul class="user-teams">
                            <% for(let i=0; i<teamMembers.length; i++) {%>

                                <li>
                                    <%= teamMembers[i].firstName %>
                                        <% if(user._id.toString()==leader._id.toString()) { %>
                                            <form style="display: inline;" method="post" action="/team/deleteMember">
                                                <input type="hidden" value="<%=teamMembers[i]._id%>" name="memberId">
                                                <input type="hidden" value="<%=team._id%>" name="teamId">
                                                <button type="submit"
                                                    style="display: inline; background:rgb(0,0,0,0);"><i
                                                        style="color: red; font-size: 20px;"
                                                        class="fa fa-close"></i></button>
                                            </form>
                                            <% } %>
                                </li>

                                <% } %>


                        </ul>

                        <% if(user._id.toString()==leader._id.toString()) { %>
                            <p>You are leader, you can remove team members by clicking on red button.</p>
                            <%}%>
                    </div>

                    <div class="content-info" style="display:none;" id="referral_panel">
                        <div class="contactedit">
                            <!-- <img style="width:100px" src="/giphy.gif" alt=""> -->
                            <div class="h1editleft"></div>
                            <h1>
                                Refferal
                            </h1>
                            <div class="h1editright"></div>
                        </div>

                        <ul class="user-teams">
                            <li>College : <%= team.college%>
                            </li>

                            <li>Refferal : <%= team._id%>
                            </li>

                            <li>
                                Sports : <%= team.eventName%>
                            </li>

                            <li>
                                Leader : <%= leader.firstName%>
                            </li>
                        </ul>
                    </div>

                    <div class="content-info" style="display:none;" id="achievements_panel">
                        <div class="contactedit">
                            <!-- <img style="width:100px" src="/giphy.gif" alt=""> -->
                            <div class="h1editleft"></div>
                            <h1>
                                Achievements
                            </h1>
                            <div class="h1editright"></div>
                        </div>

                        <div><strong>Will be after fest</strong></div>
                    </div>

                </div>
            </div>



            <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.3/jquery.min.js"
                integrity="sha512-STof4xm1wgkfm7heWqFJVn58Hm3EtS31XFaagaa8VMReCXAkQnJZ+jEy8PCC/iT18dFy95WcExNHFTqLyp72eQ=="
                crossorigin="anonymous" referrerpolicy="no-referrer"></script>
            <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
            <script>
                AOS.init();
            </script>
            <script>
                $('button').click(function () {
                    let id = "#" + $(this).attr('id') + "_panel";

                    $('.vis').toggle();
                    $('.vis').toggleClass('vis');
                    $(id).toggleClass('vis');
                    $(id).toggle();
                });
            </script>

            <script src="/js/navbar.js"></script>
</body>

</html>